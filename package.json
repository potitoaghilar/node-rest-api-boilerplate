    "@types/jsonwebtoken": "^8.5.1",
    "@types/node-fetch": "^2.5.10",
    "jest": "^26.3.0",
    "prisma": "^2.22.0",
    "ts-jest": "^26.2.0",
    "ts-node": "8.10.2",
    "ts-node-dev": "^1.0.0-pre.52",
    "types-joi": "^2.1.0",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">=12.0.0"
  }
